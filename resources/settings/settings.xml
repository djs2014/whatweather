            <listEntry value="0">@Strings.ShowInfoNothing</listEntry>
            <listEntry value="1">@Strings.ShowInfoTimeOfDay</listEntry>            
            <listEntry value="2">@Strings.ShowInfoTemperature</listEntry>
			<listEntry value="3">@Strings.ShowInfoAmbientPressure</listEntry>
			<listEntry value="4">@Strings.ShowInfoSealevelPressure</listEntry>
