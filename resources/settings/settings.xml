	<setting propertyKey="@Properties.showAlertLevel" title="@Strings.ShowAlertLevel">
		<settingConfig type="boolean" />
	</setting>
	<setting propertyKey="@Properties.showMaxPrecipitationChance" title="@Strings.ShowMaxPrecipitationChance">
		<settingConfig type="boolean" />
	</setting>	
	<setting propertyKey="@Properties.showPrecipitationChanceAxis" title="@Strings.ShowPrecipitationChanceAxis">
